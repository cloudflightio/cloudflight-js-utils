{
    "name": "cloudflight",
    "version": "0.4.0",
    "scripts": {
        "build": "nx run-many --target=build --all --parallel",
        "test": "nx run-many --target=test --all --parallel",
        "lint": "nx run-many --target=lint --all --parallel",
        "publish-workspaces": "node ./tools/scripts/prepare.js && nx run-many --target=publish --all --parallel --skip-nx-cache",
        "generate:docs": "nx run-many --target=generate-docs --all --parallel",
        "postinstall": "husky install && ngcc --properties es2020 browser module main",
        "lint:format": "nx format:check --all",
        "format:staged": "nx format:write --uncommitted",
        "docs:serve": "nx run apps-documentation:serve"
    },
    "private": true,
    "author": "Cloudflight GmbH",
    "license": "Apache-2.0",
    "keywords": [
        "js-util"
    ],
    "homepage": "https://github.com/cloudflightio/cloudflight-js-utils",
    "bugs": {
        "url": "https://github.com/cloudflightio/cloudflight-js-utils/issues"
    },
    "volta": {
        "node": "18.13.0",
        "yarn": "1.22.19"
    },
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "dependencies": {
        "@angular/animations": "15.2.5",
        "@angular/common": "15.2.5",
        "@angular/compiler": "15.2.5",
        "@angular/core": "15.2.5",
        "@angular/forms": "15.2.5",
        "@angular/platform-browser": "15.2.5",
        "@angular/platform-browser-dynamic": "15.2.5",
        "@angular/router": "15.2.5",
        "@datorama/akita": "~8.0.1",
        "@swc/helpers": "0.4.14",
        "rxjs": "~7.8.0",
        "tslib": "^2.5.0",
        "zone.js": "0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "15.2.4",
        "@angular-devkit/core": "15.2.4",
        "@angular-devkit/schematics": "15.2.4",
        "@angular/cli": "15.2.4",
        "@angular/compiler-cli": "15.2.5",
        "@angular/language-service": "15.2.5",
        "@cloudflight/eslint-plugin-angular": "0.14.1",
        "@cloudflight/eslint-plugin-typescript": "0.14.1",
        "@cloudflight/prettier-config": "0.14.1",
        "@nrwl/angular": "15.9.2",
        "@nrwl/cli": "15.9.2",
        "@nrwl/devkit": "15.9.2",
        "@nrwl/eslint-plugin-nx": "15.9.2",
        "@nrwl/jest": "16.0.1",
        "@nrwl/js": "15.9.2",
        "@nrwl/linter": "15.9.2",
        "@nrwl/nx-plugin": "15.9.2",
        "@nrwl/rollup": "15.9.2",
        "@nrwl/web": "15.9.2",
        "@nrwl/workspace": "15.9.2",
        "@rushstack/eslint-patch": "1.2.0",
        "@schematics/angular": "15.2.4",
        "@swc-node/register": "1.6.2",
        "@swc/cli": "0.1.62",
        "@swc/core": "1.3.36",
        "@types/jest": "29.5.0",
        "@types/node": "18.15.11",
        "eslint": "8.37.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-import-resolver-typescript": "3.5.4",
        "husky": "8.0.3",
        "jest": "29.5.0",
        "jest-auto-spies": "2.0.0",
        "jest-environment-jsdom": "29.5.0",
        "jest-preset-angular": "13.0.1",
        "jsonc-eslint-parser": "2.2.0",
        "lint-staged": "13.2.0",
        "ng-packagr": "15.2.2",
        "nx": "15.9.2",
        "postcss": "^8.4.21",
        "postcss-import": "15.1.0",
        "postcss-preset-env": "8.3.0",
        "postcss-url": "10.1.3",
        "prettier": "2.8.7",
        "sass-loader": "13.2.2",
        "symbol-observable": "^4.0.0",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "typedoc": "0.23.28",
        "typedoc-plugin-markdown": "3.14.0",
        "typescript": "4.9.5",
        "vuepress": "2.0.0-beta.61"
    }
}
