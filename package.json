{
  "name": "cloudflight",
  "version": "0.3.0",
  "scripts": {
    "build": "nx run-many --target=build --all --parallel",
    "test": "nx run-many --target=test --all --parallel",
    "lint": "nx run-many --target=lint --all --parallel",
    "publish-workspaces": "node ./tools/scripts/prepare.js && nx run-many --target=publish --all --parallel --skip-nx-cache",
    "generate:docs": "nx run-many --target=generate-docs --all --parallel",
    "postinstall": "husky install && ngcc --properties es2020 browser module main",
    "lint:format": "nx format:check --all",
    "format:staged": "nx format:write --uncommitted",
    "docs:serve": "nx run apps-documentation:serve"
  },
  "private": true,
  "author": "Cloudflight GmbH",
  "license": "Apache-2.0",
  "keywords": [
    "js-util"
  ],
  "homepage": "https://github.com/cloudflightio/cloudflight-js-utils",
  "bugs": {
    "url": "https://github.com/cloudflightio/cloudflight-js-utils/issues"
  },
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@angular/animations": "15.1.5",
    "@angular/common": "15.1.5",
    "@angular/compiler": "15.1.5",
    "@angular/core": "15.1.5",
    "@angular/forms": "15.1.5",
    "@angular/platform-browser": "15.1.5",
    "@angular/platform-browser-dynamic": "15.1.5",
    "@angular/router": "15.1.5",
    "@datorama/akita": "~8.0.1",
    "@swc/helpers": "0.4.14",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.6",
    "@angular-devkit/core": "15.1.6",
    "@angular-devkit/schematics": "15.1.6",
    "@angular/cli": "15.1.6",
    "@angular/compiler-cli": "15.1.5",
    "@angular/language-service": "15.1.5",
    "@cloudflight/eslint-plugin-angular": "0.9.0",
    "@cloudflight/eslint-plugin-typescript": "0.9.0",
    "@nrwl/angular": "15.7.2",
    "@nrwl/cli": "15.7.2",
    "@nrwl/devkit": "15.7.2",
    "@nrwl/eslint-plugin-nx": "15.7.2",
    "@nrwl/jest": "15.7.2",
    "@nrwl/js": "15.7.2",
    "@nrwl/linter": "15.7.2",
    "@nrwl/nx-plugin": "15.7.2",
    "@nrwl/web": "15.7.2",
    "@nrwl/workspace": "15.7.2",
    "@rushstack/eslint-patch": "1.2.0",
    "@schematics/angular": "15.1.6",
    "@swc-node/register": "1.6.2",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.36",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@vuepress/bundler-webpack": "2.0.0-beta.60",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-auto-spies": "1.6.10",
    "jest-environment-jsdom": "29.4.3",
    "jest-preset-angular": "13.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "13.1.2",
    "ng-packagr": "15.2.2",
    "nx": "15.7.2",
    "postcss": "^8.4.21",
    "postcss-import": "15.1.0",
    "postcss-preset-env": "8.0.1",
    "postcss-url": "10.1.3",
    "prettier": "2.8.4",
    "sass-loader": "13.2.0",
    "symbol-observable": "^4.0.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typedoc": "0.23.25",
    "typedoc-plugin-markdown": "3.14.0",
    "typescript": "4.9.5",
    "vuepress": "2.0.0-beta.60"
  }
}
