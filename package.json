{
    "name": "cloudflight",
    "version": "0.7.0",
    "scripts": {
        "build": "nx run-many --target=build --all --parallel",
        "test": "nx run-many --target=test --all --parallel",
        "lint": "nx run-many --target=lint --all --parallel && prettier --check .",
        "publish-workspaces": "node ./tools/scripts/prepare.js && nx run-many --target=publish --all --parallel --skip-nx-cache",
        "generate:docs": "nx run-many --target=generate-docs --all --parallel --verbose",
        "postinstall": "husky install && git update-index --chmod=+x ./.husky/pre-commit",
        "docs:serve": "vitepress serve apps/documentation/src"
    },
    "private": true,
    "author": "Cloudflight GmbH",
    "license": "Apache-2.0",
    "keywords": [
        "js-util"
    ],
    "homepage": "https://github.com/cloudflightio/cloudflight-js-utils",
    "bugs": {
        "url": "https://github.com/cloudflightio/cloudflight-js-utils/issues"
    },
    "volta": {
        "node": "20.10.0",
        "yarn": "4.0.2"
    },
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "dependencies": {
        "@angular/animations": "17.0.6",
        "@angular/common": "17.0.6",
        "@angular/compiler": "17.0.6",
        "@angular/core": "17.0.6",
        "@angular/forms": "17.0.6",
        "@angular/platform-browser": "17.0.6",
        "@angular/platform-browser-dynamic": "17.0.6",
        "@angular/router": "17.0.6",
        "@datorama/akita": "~8.0.1",
        "@swc/helpers": "0.5.3",
        "rxjs": "7.8.1",
        "tslib": "2.6.2",
        "zone.js": "0.14.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "17.0.6",
        "@angular-devkit/core": "17.0.6",
        "@angular-devkit/schematics": "17.0.6",
        "@angular/cli": "~17.0.6",
        "@angular/compiler-cli": "17.0.6",
        "@angular/language-service": "17.0.6",
        "@cloudflight/eslint-plugin-angular": "0.22.0",
        "@cloudflight/eslint-plugin-typescript": "0.22.0",
        "@cloudflight/prettier-config": "0.18.0",
        "@nx/angular": "17.2.4",
        "@nx/devkit": "17.2.4",
        "@nx/eslint": "17.2.4",
        "@nx/eslint-plugin": "17.2.4",
        "@nx/jest": "17.2.4",
        "@nx/js": "17.2.4",
        "@nx/plugin": "17.2.4",
        "@nx/vite": "17.2.4",
        "@nx/web": "17.2.4",
        "@nx/workspace": "17.2.4",
        "@rushstack/eslint-patch": "1.6.0",
        "@schematics/angular": "17.0.6",
        "@swc-node/register": "1.6.8",
        "@swc/cli": "0.1.63",
        "@swc/core": "1.3.100",
        "@swc/jest": "0.2.29",
        "@types/jest": "29.5.11",
        "@types/node": "20.10.4",
        "@vitest/coverage-istanbul": "1.0.4",
        "@vitest/ui": "1.0.4",
        "eslint": "8.55.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "happy-dom": "12.10.3",
        "husky": "8.0.3",
        "jest": "29.7.0",
        "jest-auto-spies": "3.0.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-environment-node": "29.7.0",
        "jest-preset-angular": "13.1.4",
        "jsdom": "~23.0.1",
        "jsonc-eslint-parser": "2.4.0",
        "lint-staged": "15.2.0",
        "ng-packagr": "17.0.2",
        "nx": "17.2.4",
        "postcss": "8.4.32",
        "postcss-import": "15.1.0",
        "postcss-preset-env": "9.1.4",
        "postcss-url": "10.1.3",
        "prettier": "2.8.8",
        "sass-loader": "13.3.2",
        "symbol-observable": "4.0.0",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.2",
        "typedoc": "0.24.8",
        "typedoc-plugin-markdown": "4.0.0-next.12",
        "typedoc-vitepress-theme": "1.0.0-next.1",
        "typescript": "5.2.2",
        "vite": "5.0.8",
        "vite-plugin-dts": "3.6.4",
        "vitepress": "1.0.0-rc.13",
        "vitest": "1.0.4"
    },
    "lint-staged": {
        "*": "prettier --write --ignore-unknown"
    }
}
