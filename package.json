{
    "name": "cloudflight",
    "version": "0.6.2",
    "scripts": {
        "build": "nx run-many --target=build --all --parallel",
        "test": "nx run-many --target=test --all --parallel",
        "lint": "nx run-many --target=lint --all --parallel && prettier --check .",
        "publish-workspaces": "node ./tools/scripts/prepare.js && nx run-many --target=publish --all --parallel --skip-nx-cache",
        "generate:docs": "nx run-many --target=generate-docs --all --parallel --verbose",
        "postinstall": "husky install && git update-index --chmod=+x ./.husky/pre-commit",
        "docs:serve": "vitepress serve apps/documentation/src"
    },
    "private": true,
    "author": "Cloudflight GmbH",
    "license": "Apache-2.0",
    "keywords": [
        "js-util"
    ],
    "homepage": "https://github.com/cloudflightio/cloudflight-js-utils",
    "bugs": {
        "url": "https://github.com/cloudflightio/cloudflight-js-utils/issues"
    },
    "volta": {
        "node": "18.17.1",
        "yarn": "1.22.19"
    },
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "dependencies": {
        "@angular/animations": "16.2.7",
        "@angular/common": "16.2.7",
        "@angular/compiler": "16.2.7",
        "@angular/core": "16.2.7",
        "@angular/forms": "16.2.7",
        "@angular/platform-browser": "16.2.7",
        "@angular/platform-browser-dynamic": "16.2.7",
        "@angular/router": "16.2.7",
        "@datorama/akita": "~8.0.1",
        "@swc/helpers": "0.5.2",
        "rxjs": "7.8.1",
        "tslib": "2.6.2",
        "zone.js": "0.14.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "16.2.4",
        "@angular-devkit/core": "16.2.4",
        "@angular-devkit/schematics": "16.2.4",
        "@angular/cli": "16.2.4",
        "@angular/compiler-cli": "16.2.7",
        "@angular/language-service": "16.2.7",
        "@cloudflight/eslint-plugin-angular": "0.20.5",
        "@cloudflight/eslint-plugin-typescript": "0.20.5",
        "@cloudflight/prettier-config": "0.18.0",
        "@nx/angular": "16.9.1",
        "@nx/devkit": "16.9.1",
        "@nx/eslint-plugin": "16.9.1",
        "@nx/jest": "16.9.1",
        "@nx/js": "16.9.1",
        "@nx/linter": "16.9.1",
        "@nx/plugin": "16.9.1",
        "@nx/vite": "16.9.1",
        "@nx/web": "16.9.1",
        "@nx/workspace": "16.9.1",
        "@rushstack/eslint-patch": "1.5.1",
        "@schematics/angular": "16.2.4",
        "@swc-node/register": "1.6.8",
        "@swc/cli": "0.1.62",
        "@swc/core": "1.3.91",
        "@swc/jest": "0.2.29",
        "@types/jest": "29.5.5",
        "@types/node": "18.16.3",
        "@vitest/coverage-istanbul": "0.34.6",
        "@vitest/ui": "0.34.6",
        "eslint": "8.50.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "happy-dom": "12.8.0",
        "husky": "8.0.3",
        "jest": "29.7.0",
        "jest-auto-spies": "3.0.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-environment-node": "29.7.0",
        "jest-preset-angular": "13.1.2",
        "jsdom": "~22.1.0",
        "jsonc-eslint-parser": "2.3.0",
        "lint-staged": "14.0.1",
        "ng-packagr": "16.2.3",
        "nx": "16.9.1",
        "postcss": "8.4.31",
        "postcss-import": "15.1.0",
        "postcss-preset-env": "9.1.4",
        "postcss-url": "10.1.3",
        "prettier": "2.8.8",
        "sass-loader": "13.3.2",
        "symbol-observable": "4.0.0",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "typedoc": "0.24.8",
        "typedoc-plugin-markdown": "4.0.0-next.12",
        "typedoc-vitepress-theme": "1.0.0-next.1",
        "typescript": "5.1.6",
        "vite": "4.4.9",
        "vite-plugin-dts": "3.6.0",
        "vitepress": "1.0.0-beta.1",
        "vitest": "0.34.6"
    },
    "lint-staged": {
        "*": "prettier --write --ignore-unknown"
    }
}
