{
  "projectType": "library",
  "sourceRoot": "packages/angular-logger/src",
  "prefix": "cloudflight",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/packages/angular-logger"],
      "options": {
        "project": "packages/angular-logger/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/angular-logger/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/angular-logger/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production",
      "dependsOn": ["^build"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/angular-logger"],
      "options": {
        "jestConfig": "packages/angular-logger/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/angular-logger/**/*.ts",
          "packages/angular-logger/**/*.html"
        ]
      }
    }
  },
  "tags": ["angular"]
}
